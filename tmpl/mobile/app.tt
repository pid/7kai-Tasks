<!DOCTYPE html> 
<html> 
<head> 
<title>7kai Tasks</title> 
<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="apple-mobile-web-app-status-bar-style" content="black">
<meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1, minimum-scale=1, maximum-scale=1">
<link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.13/themes/smoothness/jquery-ui.css" media="all">
<link rel="stylesheet" href="/static/css/bootstrap-1.3.0.min.css">
<link rel="stylesheet" href="/static/css/app-base.css">
<link rel="stylesheet" href="/static/css/app-mobile.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.6.4/jquery.min.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.13/jquery-ui.js"></script>
<script src="/static/js/iscroll.js"></script>
<script src="/static/js/spin.min.js"></script>
<script src="/static/js/core.js"></script>
<script src="/static/js/app-base.js"></script>
<script src="/static/js/app-constants.js"></script>
<script src="/static/js/app-base-viewer.js"></script>
<script src="/static/js/app-mobile-viewer.js"></script>
</head> 
<body>
<div id="header" class="topbar">
  <div class="fill">
    <div class="container">
      <ul class="nav">
        <li class="menu" data-setup="menu">
          <a class="menu list-name" data-setup="listname">su_aska</a>
          <ul
            class="menu-dropdown"
            style="display: none"
            data-setup="listmenu"
          >
            <li class="nosortable divider"></li>
            <li class="nosortable"><a
              data-setup="localize,click"
              data-text-ja="新しいリスト"
              data-click="createList"
            >New List</a></li>
          </ul>
        </li>
      </ul>
      <ul class="nav secondary-nav">
        <li class="menu" data-setup="menu">
          <a class="menu">menu</a>
          <ul
            class="menu-dropdown"
            style="display: none"
          >
            <!-- <li><a>Edit List</a></li>
            <li><a>Delete List</a></li>
            <li class="divider"></li>
            <li><a>Settings</a></li>
            <li><a>Help</a></li> -->
            <li><a class="action"
                data-setup="localize,switchMute"
                data-text-ja="ミュート切替"
            >Toggle Mute</a></li>
            <li><a class="action"
                data-setup="localize,switchClosed"
                data-text-ja="完了済のタスクを表示切替"
            >Toggle Show closed Task</a></li>
            <li><a class="action"
                data-setup="localize,click"
                data-text-ja="完了済のタスクを削除"
                data-click="show"
                data-show-id="clear-list-window"
            >Clear Closed Task</a></li>
            <li class="divider"></li>
            <li><a href="/signout">Sign Out</a></li>
          </ul>
        </li>
      </ul>
    </div>
  </div>
</div>
<div id="wrapper" class="container">
  <div id="scroller" data-setup="scroller">
    <ul
      class="unstyled tasks"
      data-setup="tasks,tabContent"
      data-tab-id="tasks"
      data-tab-group="main"
    >
      <li class="clearfix">
        <div class="icon sq24 icon-tasks-off"></div>
        <div class="icon sq24 icon-star-off"></div>
        <div class="human"></div>
        <div class="name">アイコン作成</div>
        <div class="icon sq24 icon-cross delete"></div>
        <div class="due">8/23</div>
        <div class="recent-comment">
          <div class="icon"></div>
          <div class="message">{<span>&nbsp;&lt;&nbsp;◯◯◯◯◯◯◯◯◯◯◯</span></div>
        </div>
      </li>
    </ul>
    <form
      id="register-task-window"
      class="form-stacked"
      style="display: none;"
      data-setup="submit,registerTaskWindow,escclose,tabContent"
      data-tab-id="registerTask"
      data-tab-group="main"
      data-submit="registerTask"
      data-show-effect="fade"
      data-show-speed="fast"
      data-show-callback="dom.autofocus"
      data-hide-effect="fade"
      data-hide-speed="fast"
      data-hide-callback="dom.blur"
    >
      <div class="modal-header">
        <h3
          data-setup="localize"
          data-text-ja="タスク登録"
        >Register Task</h3>
      </div>
      <div class="modal-body">
        <div class="row">
          <div class="left">
            <input type="hidden" name="list_id" value="">
            <input type="hidden" name="task_id" value="">
            <input type="hidden" name="registrant" value="">
            <div class="clearfix">
              <label for=""
                data-setup="localize"
                data-text-ja="タイトル (1-50文字)"
              >Title (1-50 characters)</label>
              <div class="input">
                <input class="span4" type="text" name="name" value="" required maxlength="50" data-autofocus="true">
              </div>
            </div>
            <div><a data-setup="more" data-more-id="more">More <span class="gbma"></span></a></div>
            <div id="more">
              <div class="clearfix">
                <label for=""
                  data-setup="localize"
                  data-text-ja="期日"
                >Due</label>
                <div class="input">
                  <input
                    type="text"
                    name="due"
                    value=""
                  ><br>
                  <a class="btn due-minus"><span class="icon sq24 icon-minus"></span></a>
                  <a class="btn due-plus"><span class="icon sq24 icon-plus"></span></a>
                </div>
              </div>
              <div class="clearfix">
                <label for=""
                  data-setup="localize"
                  data-text-ja="依頼者"
                >Requester</label>
                <div class="input">
                  <ul class="unstyled requester">
                    <li class="clearfix">
                      <label>
                        <div class="checkbox"><input type="radio" name="requester"></div>
                        <div class="icon"><img class="sq16"></div>
                        <div class="name">XXXX</div>
                      </label>
                    </li>
                  </ul>
                </div>
              </div>
              <div class="clearfix">
                <label
                  data-setup="localize"
                  data-text-ja="担当者"
                >Assign</label>
                <div class="input">
                  <ul class="unstyled assign">
                    <li class="clearfix">
                      <label>
                        <div class="checkbox"><input type="checkbox" name="assign"></div>
                        <div class="icon"><img class="sq16"></div>
                        <div class="name">XXXX</div>
                      </label>
                    </li>
                  </ul>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <button
          class="btn primary"
          data-setup="localize"
          data-text-ja="登録"
        >Register</button>
        <a
          class="btn secondary"
          data-setup="localize,tabMenu"
          data-text-ja="戻る"
          data-tab-id="tasks"
          data-tab-group="main"
        >Back</a>
      </div>
    </form>
    <div
      class="page"
      style="display: none;"
      data-setup="tabContent,comments,rightColumn"
      data-tab-id="comments"
      data-tab-group="main"
    >
      <h2>[<span class="list_name">-</span>] <span class="task_name">-</span></h2>
      <form
        class="form-stacked"
        style="margin-bottom: 10px;"
        data-setup="submit"
        data-submit="registerComment"
      >
        <input type="hidden" name="list_id" value="">
        <input type="hidden" name="task_id" value="">
        <input type="hidden" name="registrant" value="">
        <label
          data-setup="localize"
          data-text-ja="コメントを添える"
        >Comment on this task</label>
        <textarea
          name="message"
          style="width: 260px; height: 56px; margin-bottom: 6px;"
          required></textarea>
        <div class="">
          <button
            class="btn primary"
            data-setup="localize"
            data-text-ja="コメント"
          >Comment</button>
          <a
            class="btn secondary"
            data-setup="localize,tabMenu"
            data-text-ja="戻る"
            data-tab-id="tasks"
            data-tab-group="main"
          >Back</a>
          <a
            class="btn secondary"
            data-setup="localize,click"
            data-text-ja="編集"
            data-click="editTask"
          >Edit</a>
        </div>
      </form>
      <ul
        class="unstyled comments"
        data-setup="stretch"
        data-stretch-padding="10"
      >
        <li class="clearfix" style="position: relative;">
          <div class="icon sq32" style="position: absolute;"></div>
          <a href="#" class="icon icon-cross" style="position: absolute; right: -10px; display: block;"></a>
          <div class="" style="margin-left: 40px;">
            <div><span class="message"></span></div>
            <div><span class="date"></span><span class="name"></span></div>
          </div>
        </li>
      </ul>
    </div>
  </div>
</div>
<div id="footer" class="topbar">
  <div class="fill">
    <div class="container">
      <ul class="nav" data-setup="bottommenu">
        <li>
          <a
            data-setup="click,filterTask"
            data-click="filterTask"
            data-filter-condition='{"todo":1}'
          >
            <span
              data-setup="localize"
              data-text-ja="今日迄"
            >Todo</span>
            <span
              class="badge"
              data-setup="taskCounter"
              data-counter-condition='{"todo":1}'
            >0</span>
          </a>
        </li>
        <li>
          <a
            data-setup="click,filterTask"
            data-click="filterTask"
            data-filter-condition='{"verify":1}'
          >
            <span
              data-setup="localize"
              data-text-ja="確認待ち"
            >Verify</span>
            <span
              class="badge"
              data-setup="taskCounter"
              data-counter-condition='{"verify":1}'
            >0</span>
          </a>
        </li>
        <li class="center">
          <a
            class="icon icon-create-task"
            data-setup="click"
            data-click="createTask"
          ></a>
        </li>
        <li>
          <a
            data-setup="click,filterTask"
            data-click="filterTask"
            data-filter-condition='{"request":1}'
          >
            <span
              data-setup="localize"
              data-text-ja="依頼中"
            >Request</span>
            <span
              class="badge"
              data-setup="taskCounter"
              data-counter-condition='{"request":1}'
            >0</span>
          </a>
        </li>
        <li>
          <a
            data-setup="click,filterTask"
            data-click="filterTask"
            data-filter-condition='{"star":1}'
          >
            <span
              data-setup="localize"
              data-text-ja="スター"
            >Star</span>
            <span
              class="badge"
              data-setup="starCounter"
            >0</span>
          </a>
        </li>
      </ul>
    </div>
  </div>
</div>

[% INCLUDE "include/messages.tt" %]

<div class="icon-tasks" data-setup="hide"></div>
<div class="icon-tasks-half" data-setup="hide"></div>
<div class="icon-tasks-off" data-setup="hide"></div>
<div class="icon-star" data-setup="hide"></div>
<div class="icon-star-off" data-setup="hide"></div>
<div class="icon-plus" data-setup="hide"></div>
<div class="icon-minus" data-setup="hide"></div>

</body>
</html>