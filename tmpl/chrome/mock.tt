  <tr class="project">
    <td>サブプロジェクトX</td>
    <td><input type="checkbox"></td>
    <td></td>
    <td><span class="icon icon-delete"></span></td>
  </tr>
  <tr class="project">
    <td>[B] Project</td>
    <td><input type="checkbox"></td>
    <td></td>
