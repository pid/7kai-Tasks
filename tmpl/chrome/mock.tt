<script>
var tasks;
$(document).ready(function() {
    var localizer = new Localizer();
    if (localizer.lang != 'en') {
      localizer.localize();
    }
    tasks = new Tasks();
    tasks.localizer = localizer;
    tasks.run();
});
</script>
