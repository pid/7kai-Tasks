          <div class="input">
            <input class="xlarge" type="text" name="name" value="" required maxlength="30" data-autofocus="true">
          </div>
        </div>
        <div class="clearfix">
          <label for=""
            data-setup="localize"
            data-text-ja="期日"
          >Due</label>
          <div class="input">
            <input
              type="text"
              name="due"
              value=""
              placeholder="←↑→↓"
              data-setup="dateplus"
            >
          </div>
        </div>
        <div class="clearfix">
          <label for=""
            data-setup="localize"
            data-text-ja="依頼者"
          >Requester</label>
          <div class="input">
            <select name="requester"></select>
          </div>
        </div>
      </div>
      <div class="left" style="margin-left: 20px;">
        <div class="clearfix">
          <label for=""
            data-setup="localize"
            data-text-ja="担当者"
          >Assign</label>
          <div class="input">
            <ul class="unstyled assign">
              <li class="clearfix">
                <label>
                  <div class="checkbox"><input type="checkbox" name="assign"></div>
                  <div class="icon"><img class="sq16"></div>
                  <div class="name">XXXX</div>
                </label>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="modal-footer">
    <button
      class="btn primary"
      data-setup="localize"
      data-text-ja="登録"
    >Register</button>
    <button
      class="btn secondary"
      data-setup="localize,click"
      data-text-ja="閉じる"
      data-click="hide"
      data-hide-id="register-task-window"
    >Close</button>
  </div>
</form>

